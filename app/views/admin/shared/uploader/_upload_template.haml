{% for (var i=0, file; file=o.files[i]; i++) { %}
%tr.template-upload.fade
  %td.file-preview
    %span.preview
  %td
    %p.name
      {%=file.name%}
      %strong.error.text-danger
  %td.upload-progress
    %p.size Processing...
    .progress.progress-striped.active{role: "progressbar", aria: {valuemin: "0", valuemax: "100", valuenow: "0"}}
      .progress-bar.progress-bar-success{style: "width:0%;"}
  %td.actions
    .btn-group
      {% if (!i && !o.options.autoUpload) { %}
      %button.btn.btn-primary.start{disabled: true}
        %i.fa.fa-upload
        %span Start
      {% } %}
      {% if (!i) { %}
      %button.btn.btn-warning.cancel
        %i.fa.fa-ban-circle
        %span Cancel
      {% } %}
{% } %}
