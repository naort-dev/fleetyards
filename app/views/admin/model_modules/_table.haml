.table-responsive
  %table.table.table-hover.table-striped
    %thead
      %tr
        %th.preview
        %th=sort_link(@q, :name)
        %th=sort_link(@q, :manufacturer_name, 'Manufacturer')
        %th.actions.actions-five-ladda=I18n.t(:"labels.table.actions")
    %tbody
      = render partial: "admin/model_modules/show", collection: @model_modules, as: :model_module

      - if @model_modules.blank?
        = render "shared/tables/blank", count: 3, resource: "model_module"
